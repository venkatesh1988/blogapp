<h3>All Posts</h3>
<%= link_to "Back" , new_article_path %>

<% @articles.each do |article| %>

<p> <h4>
	<strong>Name:</strong>
	<%= link_to article.title , article %>
</p> </h4>

<p> <h5> 
	<% user = User.first %>
	<strong>Posted On: <%= Time.now%> by <%= user.name %> </strong>
	
</p> </h5>
<% end %>

