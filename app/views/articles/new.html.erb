<html>
<head>
<body>
<%= form_for :article, url: articles_path do |f| %>
<%= f.label :title %> <%= f.text_field :title%><br/>
<%= f.label :content %> <%= f.text_area :content %><br/>
<%= f.label :tag_list,"Tags (Seperated by commas)" %> <%= f.text_field :tag_list %><br/>
<%= f.submit "create", class: "btn btn-primary" %>

<% end %>
</body>
</html>